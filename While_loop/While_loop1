#include <stdio.h>
int main() {
    int n; 
    scanf("%d", &n);
    int count = 0;          
    int longest = 0;        
    int currentStreak = 0;  
    int i = 1;
    while (i <= n) {
        int noise;
        scanf("%d", &noise); 
        if(noise > 70){ 
            count++; 
            currentStreak++; 
            if(currentStreak > longest){ 
                longest = currentStreak; 
            }
        }else{
            currentStreak = 0; 
        }
        i++; 
    }
    printf("Noise Violations: %d\n", count);
    printf("Longest Violation Streak: %d\n", longest);
    return 0;
}
